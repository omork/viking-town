<pre><%= @fjord.inspect.gsub(',', ",\n") %></pre>

<%= @fjord.name %>'s villages
<% @fjord.villages.each do |village| %>
<pre><%= village.inspect.gsub(',', ",\n") %></pre>
<%= link_to village.name, :action => :show, :controller => :village, :id => village.id %>'s buildings

<% village.buildings.each do |building| %>
<pre><%= building.inspect.gsub(',', ",\n") %></pre>
<% end %>

<% end %>

<% if @fjord.villages.empty? %>
<%= render :template => 'village/found'%>
<% end %>